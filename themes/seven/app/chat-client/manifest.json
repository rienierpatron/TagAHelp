{
    "manifest_version": 2,

    "name": "Tagbond Chat",
    "description": "Tagbond CHat",
    "version": "1.0",

    "permissions": [
        "https://api.tagbond.com/",
        "https://vm:2000/",
        "http://vm:2000/"
    ],
    "browser_action": {
